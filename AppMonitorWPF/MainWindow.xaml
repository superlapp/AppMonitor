<Window x:Class="AppMonitorWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="314.5" Width="653.5">
    <Grid>
        <Button x:Name="connectToWCFBtn" Content="Connect to WCF" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="120" Height="24" Click="connectToWCFBtn_Click"/>
        <Button x:Name="getResultBtn" Content="Get result" HorizontalAlignment="Left" Margin="135,10,0,0" VerticalAlignment="Top" Width="120" Height="24" Click="getResultBtn_Click"/>
        <Button x:Name="button" Content="Get requests" HorizontalAlignment="Left" Margin="135,39,0,0" VerticalAlignment="Top" Width="120" Height="24" Click="button_Click"/>
        <ListBox x:Name="requestsListBox" HorizontalAlignment="Left" Height="206" Margin="135,68,0,0" VerticalAlignment="Top" Width="501"/>
        <Label x:Name="resultLabel" Content="Label" HorizontalAlignment="Left" Margin="260,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.311,-0.142" Height="24"/>

    </Grid>
</Window>
